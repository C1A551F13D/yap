{
	"BaseHref": "https://www.google.com",
	"IgnoreServerCertificateErrors": true,
	"UpstreamProxy": {
		"Host": "",
		"Port": 8888
	},
	"ProxyRequest": {
		"Headers": {
			"Remove": [
				"Postman-Token"
			],
			"Modify": [
				{
					"Name": "Host",
					"Value": [
						"www.google.com"
					]
				}
			]
		},
		"Body": {
			"Replace": [
				{
					"OldValue": "https://localhost:5001",
					"NewValue": "https://www.google.com"
				}
			]
		}
	},
	"ProxyResponse": {
		"Headers": {
			"Remove": [
				"Last-Modified",
				"Cache-Control"
			],
			"Modify": [
				{
					"Name": "X-Add-New-Header",
					"Value": [
						"New added values"
					]
				},
				{
					"Name": "Access-Control-Allow-Origin",
					"Value": [
						"https://www.google.com"
					]
				}
			]
		},
		"Body": {
			"Replace": [
				{
					"OldValue": "https://www.google.com",
					"NewValue": "https://localhost:5001"
				}
			]
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	}
}